{
  "prerelease": true,
  "prerelease-type": "dev",
  "versioning": "always-bump-patch",
  "bump-minor-pre-major": true,
  "bump-patch-for-minor-pre-major": true,
  "separate-pull-requests": false,
  "group-pull-request-title-pattern": "chore: release dev",
  "release-search-depth": 400,
  "skip-github-release": false,
  "always-link-local": true,
  "last-release-sha": "4853fa88f6786c82ee161e969109f169de8cd60d",
  "plugins": [
    {
      "type": "linked-versions",
      "groupName": "simba-monorepo",
      "components": ["simba", "websocket", "telemetry"]
    }
  ],
  "packages": {
    ".": {
      "release-type": "go",
      "package-name": "simba",
      "changelog-path": "CHANGELOG.md",
      "include-component-in-tag": false,
      "extra-files": []
    },
    "websocket": {
      "release-type": "go",
      "package-name": "websocket",
      "changelog-path": "CHANGELOG.md",
      "component": "websocket",
      "include-component-in-tag": true,
      "extra-files": []
    },
    "telemetry": {
      "release-type": "go",
      "package-name": "telemetry",
      "changelog-path": "CHANGELOG.md",
      "component": "telemetry",
      "include-component-in-tag": true,
      "extra-files": []
    }
  },
  "changelog-sections": [
    { "type": "feat", "section": "Features" },
    { "type": "fix", "section": "Bug Fixes" },
    { "type": "perf", "section": "Performance Improvements" },
    { "type": "revert", "section": "Reverts" },
    { "type": "docs", "section": "Documentation" },
    { "type": "refactor", "section": "Code Refactoring" },
    { "type": "chore", "hidden": true },
    { "type": "style", "hidden": true },
    { "type": "test", "hidden": true },
    { "type": "build", "hidden": true },
    { "type": "ci", "hidden": true }
  ]
}
